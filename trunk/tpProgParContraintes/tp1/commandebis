commandeBis(NbResistance,NbCondensateur) :- 	nbMaxR(NbMaxR),nbMinR(NbMinR),nbMaxC(NbMaxC),nbMinC(NbMinC),
						NbResistance>=NbCondensateur,
						isBetween(NbResistance,NbMinR,NbMaxR),
						isBetween(NbCondensateur,NbMinC,NbMaxC).
						

